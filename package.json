{
  "name": "vintedgoat",
  "version": "1.0.0",
  "description": "Outil d'analyse pour boutiques Vinted",
  "main": "src/app.js",
  "scripts": {
    "start": "parcel index.html",
    "build": "parcel build index.html",
    "lint": "eslint src/**/*.js",
    "test": "jest"
  },
  "dependencies": {
    "chart.js": "^3.7.0",
    "lodash": "^4.17.21"
